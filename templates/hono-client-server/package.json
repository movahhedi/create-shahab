	"contributors": [
		{
			"name": "<FILL>",
			"email": "<FILL>",
			"url": "<FILL>"
		}
	],
	"type": "module",
	"main": "client/src/index.html",
	"directories": {
		"doc": "docs"
	},
	"scripts": {
		"i": "yarn install:root ; yarn install:client ; yarn install:server ; yarn install:shared",
		"postinstall": "yarn install:client ; yarn install:server ; yarn install:shared",
		"install:root": "yarn install",
		"install:client": "cd client && yarn install",
		"install:server": "cd server && yarn install",
		"install:shared": "cd shared && yarn install",
		"prepare": "yarn prepare:client ; yarn prepare:server",
		"prepare:client": "cd client && yarn prepare",
		"prepare:server": "cd server && yarn prepare",
		"prepare:shared": "cd shared && yarn prepare",
		"start": "concurrently -c \"bgBlue,bgGreen\" \"npm:client\" \"npm:server\"",
		"dev": "cd client && npx vite",
		"client": "cd client && npx vite",
		"server": "cd server && yarn start",
		"server-php": "cd server-php && php -S 0.0.0.0:8000 -t=\"./api\"",
		"build": "yarn build:client ; yarn build:server",
		"build2": "yarn build:client",
		"build:client": "cd client && yarn build",
		"build:server": "cd server && yarn build",
		"prod": "yarn production",
		"production": "yarn production:client ; yarn production:server",
		"production:client": "cd client && yarn production",
		"production:server": "cd server && yarn production",
		"types": "yarn types:client ; yarn types:server ; yarn types:shared",
		"types:client": "cd client && yarn types",
		"types:server": "cd server && yarn types",
		"types:shared": "cd shared && yarn types",
		"lint": "concurrently -m 1 -c \"bgBlue,bgYellow,bgGreen\" \"npm:lint:*\"",
		"lint:root": "eslint . --fix --c \"eslint.config.mjs\" --parser-options project:tsconfig.eslint.json",
		"lint:client": "cd client && yarn lint",
		"lint:server": "cd server && yarn lint",
		"lint:shared": "cd shared && yarn lint",
		"format": "concurrently -m 1 -c \"bgBlue,bgYellow,bgGreen\" \"npm:format:*\"",
		"format:client": "cd client && yarn format",
		"format:server": "cd server && yarn format",
		"format:shared": "cd shared && yarn format",
		"fl": "concurrently -m 1 -c \"bgGreen,bgMagenta\" \"npm:format\" \"npm:lint\"",
		"fl:styles": "cd client && yarn fl:styles",
		"fl:scripts": "cd client && yarn fl:scripts",
		"fl:server": "cd server && yarn fl",
		"fl:shared": "cd shared && yarn fl",
		"update": "yarn update:root ; yarn update:client ; yarn update:server ; yarn update:shared",
		"update:root": "npx npm-check-updates -i",
		"update:client": "cd client && yarn update",
		"update:server": "cd server && yarn update",
		"update:shared": "cd shared && yarn update",
		"update:yarn": "yarn set version stable",
		"clear": "yarn clear:client ; yarn clear:server",
		"clear:client": "cd client && yarn clear",
		"clear:server": "cd server && yarn clear",
		"clear:shared": "cd shared && yarn clear",
		"clear:nodeModules": "yarn clear:nodeModules:root ; yarn clear:nodeModules:client ; yarn clear:nodeModules:server ; yarn clear:nodeModules:shared",
		"clear:nodeModules:root": "rimraf -G nodeModules",
		"clear:nodeModules:client": "cd client && yarn clear:nodeModules",
		"clear:nodeModules:server": "cd server && yarn clear:nodeModules",
		"clear:nodeModules:shared": "cd shared && yarn clear:nodeModules"
	},
	"dependencies": {
		"hono": "^4.4.8"
	},
	"devDependencies": {
		"@eslint/js": "^9.5.0",
		"@movahhedi/eslint-config": "^0.1.4",
		"concurrently": "^8.2.2",
		"cspell-define-config": "^0.1.1",
		"eslint": "^9.5.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-define-config": "^2.1.0",
		"eslint-plugin-import": "^2.29.1",
		"eslint-plugin-jsonc": "^2.16.0",
		"eslint-plugin-package-json": "^0.15.2",
		"eslint-plugin-perfectionist": "^2.11.0",
		"eslint-plugin-prettier": "^5.1.3",
		"eslint-plugin-react": "^7.34.3",
		"eslint-plugin-redos": "^4.4.5",
		"jsonc-eslint-parser": "^2.4.0",
		"npm-check-updates": "^16.14.20",
		"postcss": "^8.4.38",
		"prettier": "^3.3.2",
		"prettier-define-config": "^1.0.1",
		"prettier-eslint-cli": "^8.0.1",
		"rimraf": "^5.0.7",
		"stylelint": "^16.6.1",
		"stylelint-config-standard-scss": "^13.1.0",
		"stylelint-define-config": "^1.5.0",
		"stylelint-order": "^6.0.4",
		"stylelint-scss": "^6.3.2",
		"ts-plugin-sort-import-suggestions": "^1.0.4",
		"typescript": "^5.5.2",
		"typescript-eslint": "^7.14.1",
		"typescript-plugin-css-modules": "^5.1.0"
	},
	"packageManager": "yarn@4.3.1"
}
