{
	"compilerOptions": {
		"noImplicitAny": false,
		"lib": ["ESNext"],
		"target": "ESNext",
		"module": "ESNext",
		"moduleResolution": "Node",
		"outDir": "dist",
		"removeComments": false,
		"preserveConstEnums": true,
		"sourceMap": true,
		"skipLibCheck": true,
		"esModuleInterop": true,
		"resolveJsonModule": true,
		"allowSyntheticDefaultImports": true,
		"allowImportingTsExtensions": false,
		"noEmit": false,
		"declaration": true,
		"strict": true
	}
}
